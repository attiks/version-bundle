{
  "name": "netbrothers-gmbh/version-bundle",
  "description": "SymfonyBundle for handling versions based on triggers in mysql with doctrine",
  "version": "0.0.2",
  "keywords": ["symfony","bundle","versioning","doctrine","mysql"],
  "type": "symfony-bundle",
  "require": {
    "php": ">=7.2.5",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/monolog-bundle": "*",
    "symfony/orm-pack": "*"
  },
  "autoload": {
    "psr-4": {
      "NetBrothers\\VersionBundle\\": "src/"
    }
  },
  "autoload-dev": {
        "psr-4": {
            "NetBrothers\\VersionBundle\\Tests\\": "tests/"
        }
    },
  "license": "MIT",
  "authors": [
